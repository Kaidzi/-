operation=input ('операция:')
if operation =="рассчитать силу тока" or operation =='1':
    x=float (input('введите напряжение (В): '))
    y = float(input('введите сопротивление (Ом): '))
    result=x/y
    print('Сила тока равна: ', result,'A')
elif operation=='рассчитать силу Архимеда 'or operation =='2':
    x = float(input('введите плотность жидкости (кг/М^3): '))
    y = float(input('введите объём тела,погруженного в жидкость (M^3) : '))
    z = float(input('ведите ускорение свободного падения (H/кг) : '))
    result = x*y*z
    print('Сила Архимеда равна: ',result,'H')
elif operation == 'рассчитать силу упругости тела'or operation =='3':
    x = float(input('введите жёсткость тела (H/M) : '))
    y = float(input('введите изменение длины тела (M) : '))
    result = x*y
    print('Сила упругости равна: ', result,'H')
elif operation == 'рассчитать работу'or operation =='4':
    x = float(input('введите силу,приложенное к телу (Н) : '))
    y = float(input('введите расстояние пройденное телом (M) : '))
    result = x * y
    print('Работа равна: ', result, 'Дж')
elif operation == 'рассчитать вес тела в жидкости'or operation =='5':
    x = float(input('введите вес тела в воздухе (Н) : '))
    y = float(input('введите силу Архимеда (M) : '))
    result = x - y
    print('Вес тела в жидкости равен: ', result, 'H')
elif operation == 'рассчитать плотность тела'or operation =='6':
    x = float(input('введите массу тела (Кг) : '))
    y = float(input('введите объём тела (M^3) : '))
    result = x / y
    print('Плотность тела равна: ', result, 'Кг/М^3')
elif operation == 'рассчитать массу тела'or operation =='7':
    x = float(input('введите плотность тела (Кг/М^3) : '))
    y = float(input('введите объём тела (M^3) : '))
    result = x * y
    print('Масса тела равна: ', result, 'Кг')
elif operation == 'рассчитать силу тяжести'or operation =='8':
    x = float(input('введите массу тела (Кг) : '))
    y = float(input('введите ускорение свободного падения (Н/Кг) : '))
    result = x * y
    print('Сила тяжести равна: ', result, 'Н')
elif operation == 'рассчитать силу ,приложенную к блоку'or operation =='9':
    x = float(input('введите массу тела (Кг) : '))
    y = float(input('введите ускорение свободного падения (Н/Кг) : '))
    z = float(input('введите кол-во блоков : '))
    result = (x * y)/z
    print('Сила, приложенная к блоку равна: ', result, 'Н')
elif operation == 'рассчитать КПД'or operation =='10':
    x = float(input('введите полезную работу (Дж) : '))
    y = float(input('введите полную работу (Дж) : '))
    result = x / y * 100
    print('КПД равен: ', result, '%')
elif operation == 'рассчитать выделяемую теплоту'or operation =='11':
    x = float(input('введите массу тела (Кг) : '))
    y = float(input('введите удельную теплоёмкость вещества (Дж/Кг*С) : '))
    z = float(input('введите изменение температуры (С) : '))
    result = x *y*z
    print('Выделяемая теплота равна: ', result, 'Дж')
elif operation == 'рассчитать выделяемую теплоту при сгорании топлива'or operation =='12':
    x = float(input('введите массу тела (Кг) : '))
    y = float(input('введите удельную теплоту сгорания топлива (Дж/Кг) : '))
    result = x * y
    print('Выделяемая теплота при сгорании топлива равна: ', result, 'Дж')
elif operation == 'рассчитать выделяемую теплоту при плавлении'or operation =='13':
    x = float(input('введите массу тела (Кг) : '))
    y = float(input('введите удельную теплоту плавления (Дж/Кг) : '))
    result = x * y
    print('Выделяемая теплота при плавлении равна: ', result, 'Дж')
elif operation == 'рассчитать выделяемую теплоту при парообразовании' or operation == '14':
    x = float(input('введите массу тела (Кг) : '))
    y = float(input('введите удельную теплоту парообразования (Дж/Кг) : '))
    result = x * y
    print('Выделяемая теплота при парообразовании равна: ', result, 'Дж')
elif operation == 'рассчитать напряжение' or operation == '15':
    x = float(input('введите работу электрической силы (Дж) : '))
    y = float(input('введите заряд, перемещённый по проводнику из одной точки в другую (Кулон) : '))
    result = x / y
    print('Напряжение равно: ', result, 'В')
elif operation == 'рассчитать сопротивление' or operation == '16':
    x = float(input('введите длину проводника (М) : '))
    y = float(input('введите удельное сопротивление (Ом*М) : '))
    z = float(input('введите площадь поперечного сечения (мм^2) : '))
    result = (x * y)/z
    print('Сопротивление равно: ', result, 'Ом')
elif operation == 'рассчитать работу электрического тока' or operation == '17':
    x = float(input('введите силу тока (А) : '))
    y = float(input('введите напряжение (В) : '))
    z = float(input('введите время(с) : '))
    result = x * y * z
    print('Работа равна: ', result, 'Дж')
elif operation == 'рассчитать выделяемую теплоту по закону Джоуля-Ленца' or operation == '18':
    x = float(input('введите силу тока (А) : '))
    y = float(input('введите сопротивление (Ом) : '))
    z = float(input('введите время(с) : '))
    result = x*x * y * z
    print('Выделяаемая теплота по закону Джоуля-Ленца равна: ', result, 'Дж')
elif operation == 'рассчитать мощность электрического тока' or operation == '19':
    x = float(input('введите силу тока (А) : '))
    y = float(input('введите напряжение (В) : '))
    result = x*y
    print('Мощность электрического тока равна: ', result, 'Вт')
elif operation == 'рассчитать конечную скорость при равноускоренном движении' or operation == '20':
    x = float(input('введите начальную скорость (м/с) : '))
    y = float(input('введите ускорение (м/с^2) : '))
    z = float(input('введите время(с) : '))
    result = x + y*z
    print('Конечная скорость при равноускоренном движении равна: ', result, 'м/c')
elif operation == 'рассчитать перемещение' or operation == '21':
    x = float(input('введите скорость (м/с) : '))
    y = float(input('введите время(с) : '))
    result = x * y
    print('Перемещение равно: ', result, 'М')
elif operation == 'рассчитать перемещение при равноускоренном движении' or operation == '22':
    x = float(input('введите начальную скорость (м/с) : '))
    y = float(input('введите ускорение (м/с^2) : '))
    z = float(input('введите время(с) : '))
    result = x * z+ (y*z*z)/2
    print('Перемещение при равноускоренном движении равно: ', result, 'М')

elif operation == 'найди площадь треугольника'or operation == '23':
   a = float(input('Введите значение длины одной из сторон: '))
   h = float(input('Введите значение длины высоты, проведенной к этой стороне: '))
   print('Площадь треугольника равна ', a * h / 2, '.')

elif operation == 'найди площадь параллелограмма'or operation == '24':
   a = float(input('Введите значение длины одной из сторон: '))
   h = float(input('Введите значение длины высоты, проведенной к этой стороне: '))
   print('Площадь параллелограмма равна ', a * h, '.')

elif operation == 'найди площадь прямоугольника'or operation == '25':
   a = float(input('Введите значение длины первой стороны: '))
   b = float(input('Введите значение длины второй стороны: '))
   print('Площадь прямоугольника равна ', a * b, '.')

elif operation == 'найди площадь трапеции'or operation == '26':
   a = float(input('Введите значение длины одного из оснований: '))
   b = float(input('Введите значение длины другого основания: '))
   h = float(input('Введите значение длины высоты: '))
   result = (a + b) / 2 * h
   print('Площадь равна ',result, '.')

elif operation == 'найди площадь квадрата'or operation == '27':
   a = float(input('Введите значение длины одной из сторон: '))
   print('Площадь квадрата равна ', a * a, '.')

elif operation == 'найди площадь окружности'or operation == '28':
  r = float(input('Введите значение длины радиуса: '))
  print('Площадь окружности равна ', r * r * 3.14, '.')

elif operation == 'найди длину окружности'or operation == '29':
  r = float(input('Введите значение длины радиуса: '))
  print('Длина окружности равна ', r * 2 * 3.14, '.')

elif operation == 'найди периметр треугольника'or operation == '30':
  a = float(input('Введите значение длины первой стороны: '))
  b = float(input('Введите значение длины второй стороны: '))
  c = float(input('Введите значение длины третьей стороны: '))
  print('Периметр треугольника равен', a + b + c, '.')

elif operation == 'найди периметр квадрата'or operation == '31':
  a = float(input('Введите значение длины одной из сторон: '))
  print('Периметр квадрата равен', a * 4, '.')

elif operation == 'найди периметр параллелограмма'or operation == '32':
  a = float(input('Введите значение длины одной из сторон: '))
  b = float(input('Введите значение длины стороны, смежной с первой: '))
  print('Периметр параллелограмма равен', (a + b) * 2, '.')

elif operation == 'переведи из мм'or operation == '33':
     x = float(input('Введите значение в мм: '))
     result = x*0.001
     print('Ответ в [СИ]', result, '.')
elif operation == 'переведи из см 'or operation == '34':
