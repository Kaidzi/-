    result = x * 0.01
    print('Ответ в [СИ]', result, '.')
elif operation == 'переведи из м2'or operation == '41':
    x = float(input('Введите значение в м2: '))
    result = x * 1
    print('Ответ в [СИ]', result, '.')
elif operation == 'переведи из а'or operation == '42':
    x = float(input('Введите значение в а: '))
    result = x * 100
    print('Ответ в [СИ]', result, '.')
elif operation == 'переведи из га'or operation == '43':
    x = float(input('Введите значение в га: '))
