    result = x * 1
    print('Ответ в [СИ]', result, '.')
elif operation == 'переведи из км 'or operation == '37':
    x = float(input('Введите значение в км: '))
    result = x * 1000
    print('Ответ в [СИ]', result, '.')
elif operation == 'переведи из мм2 'or operation == '38':
    x = float(input('Введите значение в мм2: '))
    result = x * 0.000001
    print('Ответ в [СИ]', result, '.')
elif operation == 'переведи из см2'or operation == '39':
