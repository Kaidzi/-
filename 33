    print('Ответ в [СИ]', result, '.')
elif operation == 'переведи из км2'or operation == '44':
    x = float(input('Введите значение в км2: '))
    result = x * 1000000
    print('Ответ в [СИ]', result, '.')
elif operation == 'переведи из мм3'or operation == '45':
    x = float(input('Введите значение в мм3: '))
    result = x * 0.000000001
    print('Ответ в [СИ]', result, '.')
elif operation == 'переведи из см3'or operation == '46':
    x = float(input('Введите значение в см3: '))
    result = x * 0.000001
